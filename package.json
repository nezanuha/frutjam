{
  "name": "frutjam",
  "version": "1.6.0-beta.0",
  "description": "A utility-first CSS framework UI Library",
  "funding": "https://ko-fi.com/nezanuha",
  "main": "./dist/frutjam.css",
  "browser": "dist/frutjam.css",
  "style": "./dist/frutjam.css",
  "files": [
    "src/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nezanuha/frutjam.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --mode=production",
    "prepublishOnly": "npm run build",
    "dev": "webpack serve --mode=development"
  },
  "exports": {
    ".": "./src/frutjam.css",
    "./components/*": "./src/components/*",
    "./themes/*": "./src/theme/*",
    "./utilities/*": "./src/utilities/*"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "keywords": [
    "tailwindcss",
    "UI Library"
  ],
  "author": "Nezanuha",
  "license": "MIT",
  "homepage": "https://github.com/nezanuha/frutjam",
  "bugs": {
    "url": "https://github.com/nezanuha/frutjam/issues"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "tailwindcss": "^4.1.10"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cssnano": "^7.0.7",
    "glob": "^11.0.3",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.1.1",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
