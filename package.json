{
  "name": "frutjam",
  "version": "1.0.0",
  "description": "A utility-first CSS framework UI Library",
  "funding": "https://ko-fi.com/nezanuha",
  "main": "./dist/frutjam.local.css",
  "browser": "dist/frutjam.local.css",
  "style": "./dist/frutjam.local.css",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nezanuha/frutjam.git"
  },
  "exports": {
    ".": "./dist/frutjam.local.css",
    "./safelist": "./dist/frutjam.safelist.local.css"
  },
  "scripts": {
    "clean": "rimraf  ./dist",
    "dev": "webpack serve --config scripts/webpack.config.dev.js --mode=development",
    "build:local": "node scripts/build-local.mjs && node scripts/build-safelist-local.mjs",
    "build:browser": "webpack --config scripts/webpack.config.prod.js --mode=production",
    "build": "npm run clean && npm run build:browser && npm run build:local",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "keywords": [
    "frutjam",
    "tailwindcss",
    "UI Library",
    "tailwind",
    "taiwind css",
    "components",
    "tailwind components",
    "tailwind component library",
    "tailwind ui",
    "tailwind ui library",
    "tailwind next",
    "tailwind nuxt",
    "tailwind svelte",
    "tailwind astro",
    "tailwind django",
    "tailwind rails",
    "tailwind laravel",
    "tailwind css plugin",
    "component library",
    "tailwind themes",
    "css library",
    "theme",
    "front-end",
    "framework",
    "W3C standards",
    "accessibility web development",
    "seo friendly ui",
    "rtl ui",
    "responsive ui library"
  ],
  "author": "Nezanuha",
  "license": "MIT",
  "homepage": "https://frutjam.com",
  "bugs": {
    "url": "https://github.com/nezanuha/frutjam/issues"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "chalk": "^5.6.2",
    "css-loader": "^7.1.2",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.2.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rimraf": "^6.1.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-remove-empty-scripts": "^1.1.1"
  }
}
