{
  "name": "frutjam",
  "version": "0.1.0-alpha.1",
  "description": "A utility-first CSS framework UI Library",
  "main": "./dist/frutjam.css",
  "browser": "dist/frutjam.css",
  "style": "./dist/frutjam.css",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nezanuha/frutjam.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --mode=production",
    "prepublishOnly": "npm run build",
    "dev": "webpack serve --mode=development"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "exports": {
    "./base/*": "./src/base/*",
    "./components/*": "./src/components/*",
    "./themes/*": "./src/themes/*"
  },
  "keywords": [
    "tailwindcss",
    "UI Library"
  ],
  "author": "Nezanuha",
  "license": "MIT",
  "homepage": "https://github.com/nezanuha/frutjam",
  "bugs": {
    "url": "https://github.com/nezanuha/frutjam/issues"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.12",
    "tailwindcss": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "rimraf": "^6.0.1",
    "terser-webpack-plugin": "^5.3.13",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
