{
  "name": "frutjam",
  "version": "0.8.1-beta.6",
  "description": "A utility-first CSS framework UI Library",
  "main": "./dist/frutjam.css",
  "browser": "dist/frutjam.css",
  "style": "./dist/frutjam.css",
  "files": [
    "src/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nezanuha/frutjam.git"
  },
  "exports": {
    ".": "./src/main.css",
    "./base/*": "./src/base/*",
    "./components/*": "./src/components/*",
    "./themes/*": "./src/themes/*",
    "./utilities/*": "./src/utilities/*"
  },
  "scripts": {
    "dev": "webpack serve --mode=development"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "keywords": [
    "tailwindcss",
    "UI Library"
  ],
  "author": "Nezanuha",
  "license": "MIT",
  "homepage": "https://github.com/nezanuha/frutjam",
  "bugs": {
    "url": "https://github.com/nezanuha/frutjam/issues"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "tailwindcss": "^4.1.7"
  },
  "devDependencies": {
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  }
}
